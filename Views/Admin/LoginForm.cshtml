@{
    ViewData["Title"] = "FacultyForm";
}
@model WebApplication1.Models.Admin

<form method="post" asp-action="LoginForm">
    <div class=" col-md-4  mx-4" style="width:50%;left:0">
        <div class="form-group my-2">
            <label class="text-muted" asp-for="@Model.AdminUserName"> </label>
            <input type="text" class="form-control" asp-for="@Model.AdminUserName" />
        </div>
        <div class="form-group">
            <label class="text " asp-for="@Model.AdminPassword"> </label>
            <input type="password" class="form-control" asp-for="@Model.AdminPassword" />
        </div>
    </div>
    <div class="custom-file mx-3 my-3">
        <button type="submit" class=" btn btn-outline-primary ">
            <i class="bi bi-check"></i>
            submit
        </button>
        <a href="~/Home" class="btn btn-outline-secondary ">
            <i class="bi bi-x"></i>
            Cancle
        </a>
    </div>
</form>